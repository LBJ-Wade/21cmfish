# Config file for ETHOS runs

[run]
name : quick
#output_dir : /h/hobnob0/Reionization_models/21cmFAST-ethos-analysis/21cmFAST_notebooks/_cache/big_box/fisher/quick/
output_dir : /Users/cmason/Documents/Research/21cmFAST/21cmFAST-ethos-analysis/21cmFAST_notebooks/_cache/big_box/fisher/quick/

# redshifts
[redshifts]
min : 5.
max : 10.

# UserParam
[user_params]
HII_DIM : 64
BOX_LEN : 200
USE_FFTW_WISDOM : True
USE_INTERPOLATION_TABLES : True
FAST_FCOLL_TABLES : False
USE_RELATIVE_VELOCITIES : False
POWER_SPECTRUM : 0

# AstroParam
[astro_params]
ALPHA_ESC: -0.5
F_ESC10 : -1.
ALPHA_STAR : 0.5
F_STAR10 : -1.3
t_STAR : 0.5
M_TURN : 8.7
L_X : 40.5
NU_X_THRESH : 500.0

# FlagOptions
[flag_options]
INHOMO_RECO : True
USE_MASS_DEPENDENT_ZETA : True
USE_TS_FLUCT : True
USE_MINI_HALOS : False
FIX_VCB_AVG : False
USE_ETHOS : False
FILTER : 0

# Parameters to vary in the Fisher analysis
[vary]
astro_params_vary :
  ALPHA_STAR
